package game;

public class cityFactory {
	
	public City makeCity(char c)
	{
		City city = null;
		switch(c) {
			case 'A' :
				city=new CityA("A");
				break;
			case 'B' :
				city=new CityB("B");
				break;
			case 'C' :
				city=new CityC("C");
				break;
			case 'D' :
				city=new CityD("D");
				break;
			case 'E' :
				city=new CityE("E");
				break;
			case 'F' :
				city=new CityF("F");
				break;
			case 'G' :
				city=new CityG("G");
				break;
			case 'H' :
				city=new CityH("H");
				break;
			case 'I' :
				city=new CityI("I");
				break;
			case 'J' :
				city=new CityJ("J");
				break;
			case 'K' :
				city=new CityK("K");
				break;
			case 'L' :
				city=new CityL("L");
				break;
		}
		
		return city;
		
	}
}
