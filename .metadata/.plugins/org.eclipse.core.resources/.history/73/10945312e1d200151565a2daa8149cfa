package game;

import javax.swing.JButton;

public class City extends JButton{
	
	String name;
	int cost;
	int building_count;
	User owner;
	public City(String name) {
		// TODO Auto-generated constructor stub
		super(name);
		this.name=name;
		building=0;
	}
	
	public String getName()
	{
		return name;
	}
	public int getCost()
	{
		return cost;
	}
	public int getBuilding()
	{
		return building;
	}
	public User getOwner()
	{
		return owner;
	}
}
