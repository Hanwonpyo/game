package game;

public class cityFactory {
	
	public City makeCity(String s)
	{
		City city = null;
		switch(c) {
			case 'A' :
				city=new CityA(s);
				break;
			case 'B' :
				city=new CityB(s);
				break;
			case 'C' :
				city=new CityC(s);
				break;
			case 'D' :
				city=new CityD(s);
				break;
			case 'E' :
				city=new CityE(s);
				break;
			case 'F' :
				city=new CityF(s);
				break;
			case 'G' :
				city=new CityG(s);
				break;
			case 'H' :
				city=new CityH(s);
				break;
			case 'I' :
				city=new CityI(s);
				break;
			case 'J' :
				city=new CityJ(s);
				break;
			case 'K' :
				city=new CityK(s);
				break;
			case 'L' :
				city=new CityL(s);
				break;
		}
		return city;
		
	}
}
