package game;

public class cityFactory {
	
	public City makeCity(char c)
	{
		City city = null;
		switch(n) {
			case 1 :
				city=new CityA("A");
				break;
			case 2 :
				city=new CityB("B");
				break;
			case 3 :
				city=new CityC("C");
